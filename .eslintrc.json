{
  "extends": "airbnb",
  "rules": {
    "object-curly-spacing": "off",
    "object-curly-spacing": "off",
    "prefer-arrow-callback": "off",
    "no-throw-literal": "off",
    "object-shorthand": "off",
    "consistent-return": "off",
    "comma-dangle": "off",
    "key-spacing": "off",
    "prefer-const": "off",
    "padded-blocks": "off",
    "prefer-template": "off",
    "vars-on-top": "off"
  },
  "globals": {
    "_require": true
  }
}
